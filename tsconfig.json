{
  "compilerOptions": {
    "target": "ESNext",
    "strict": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "components"
  ],
  "exclude": [
    "node_modules"
  ]
}
